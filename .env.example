# ============================================================
# 은둔마을 — 환경 변수 예시 (.env.example)
# ============================================================
# 이 파일을 복사해 .env 를 만들고 값을 채우세요.
# .env 는 저장소에 커밋하지 마세요. EAS 빌드 시에는 EAS Secrets 사용 권장.
# ============================================================

# --- Supabase (필수) ---
EXPO_PUBLIC_SUPABASE_URL=
EXPO_PUBLIC_SUPABASE_ANON_KEY=

# --- Supabase CLI / 로컬 개발 (마이그레이션·함수 배포 등) ---
SUPABASE_ACCESS_TOKEN=
SUPABASE_DB_PASSWORD=
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# --- Anthropic (선택, 감정 분석 Edge Functions 사용 시) ---
ANTHROPIC_API_KEY=

# --- Sentry (선택, 프로덕션 에러 수집, 상세: docs/SENTRY.md) ---
EXPO_PUBLIC_SENTRY_DSN=
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=
SENTRY_DISABLE_AUTO_UPLOAD=false
# 참고용 (현재 앱/EAS에서 미사용)
SENTRY_PROJECT_SLUG=
SENTRY_ORGANIZATION_ID=
SENTRY_PROJECT_ID=
SENTRY_PERSONAL_TOKEN=
SENTRY_SECURITY_TOKEN=

# --- E2E 테스트 (Maestro) ---
MAESTRO_ADMIN_EMAIL=
MAESTRO_ADMIN_PASSWORD=
# MAESTRO_CLEAR_APP_DATA=false   # 기본값 true, 앱 데이터 초기화 비활성화 시
